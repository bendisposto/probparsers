RULES_MACHINE FunctionDependencies
OPERATIONS
COMPUTATION COMP_comp1
BODY
  DEFINE
    xx
  TYPE
    POW(INTEGER)
  VALUE
    {1}
  END//DEFINEskip
END//COMPUTATION
;
FUNCTION out <-- FUNC_Add
DEPENDS_ON_COMPUTATION COMP_comp1
BODY
  out := xx \/ {2}
END//FUNCTION
;
COMPUTATION COMP_comp2
BODY
  DEFINE
    yy
  TYPE
    POW(INTEGER)
  VALUE
    {3}
  END//DEFINEskip
END//COMPUTATION
;
FUNCTION out <-- FUNC_Add2
DEPENDS_ON_COMPUTATION COMP_comp2
BODY
  out <-- FUNC_Add;
  out := out \/ yy
END//FUNCTION

