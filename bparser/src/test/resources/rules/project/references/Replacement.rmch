RULES_MACHINE Replacement
OPERATIONS

COMPUTATION COMP_comp1
BODY
  DEFINE
    xx
  TYPE
    POW(INTEGER)
  VALUE
    {}
  END//DEFINE
END//COMPUTATION
;
COMPUTATION COMP_comp2
DEPENDS_ON_COMPUTATION COMP_comp1
BODY
  DEFINE
    yy
  TYPE
    POW(INTEGER)
  VALUE
    {1,2,3} \/ xx
  END//DEFINE
END//COMPUTATION
;
COMPUTATION COMP_comp3
DEPENDS_ON_COMPUTATION COMP_comp2
BODY
  DEFINE
    zz
  TYPE
    POW(INTEGER)
  VALUE
    {}
  END//DEFINE
END//COMPUTATION
;
COMPUTATION COMP_comp4
DEPENDS_ON_COMPUTATION COMP_comp3
BODY
  DEFINE
    aa
  TYPE
    POW(INTEGER)
  VALUE
    {1,2,3,4} \/ zz
  END//DEFINE
END//COMPUTATION
;
COMPUTATION COMP_comp2New
REPLACES COMP_comp2
DEPENDS_ON_COMPUTATION COMP_comp1
BODY
  DEFINE
    yy
  TYPE
    POW(INTEGER)
  VALUE
    {1,2,3,4} \/ xx
  END//DEFINE
END//COMPUTATION

